    {
      "provider": "cdnjs",
      "library": "modernizr@2.8.3",
      "destination": "wwwroot/lib/modernizr/",
      "files": [
        "modernizr.min.js"
      ]
    },
    {
      "provider": "unpkg",
      "library": "@microsoft/signalr@3.1.5",
      "destination": "wwwroot/lib/signalr/",
      "files": [
        "dist/browser/signalr.js",
        "dist/browser/signalr.js.map",
        "dist/browser/signalr.min.js"
      ]
    },
    {
      "provider": "unpkg",
      "library": "moment@2.27.0",
      "destination": "wwwroot/lib/moment/",
      "files": [
        "min/moment.min.js",
        "min/moment.min.js.map"
      ]
    },
    {
      "provider": "unpkg",
      "library": "bootstrap@4.5.0",
      "destination": "wwwroot/lib/bootstrap/dist/",
      "files": [
        "css/bootstrap.css",
        "css/bootstrap.css.map",
        "js/bootstrap.js",
        "js/bootstrap.js.map",
        "js/bootstrap.min.js",
        "js/bootstrap.bundle.js",
        "js/bootstrap.bundle.min.js"
      ]
    },
    {
      "provider": "unpkg",
      "library": "jquery@3.5.1",
      "destination": "wwwroot/lib/jquery/dist/",
      "files": [ "dist/jquery.js", "dist/jquery.min.js", "dist/jquery.min.map" ]
    },
    {
      "provider": "unpkg",
      "library": "jquery-validation@1.19.2",
      "destination": "wwwroot/lib/jquery-validation/",
      "files": [ "dist/additional-methods.min.js", "dist/jquery.validate.min.js" ]
    },
    {
      "provider": "cdnjs",
      "library": "jquery-validation-unobtrusive@3.2.11",
      "files": [ "jquery.validate.unobtrusive.js", "jquery.validate.unobtrusive.min.js" ],
      "destination": "wwwroot/lib/jquery-validation-unobtrusive/"
